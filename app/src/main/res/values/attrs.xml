<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--验证码的属性-->
    <declare-styleable name="VerificationCodeEditText">
        <attr name="figures" format="integer" />
        <attr name="verCodeMargin" format="dimension" />
        <attr name="bottomLineSelectedColor" format="reference" />
        <attr name="bottomLineNormalColor" format="reference" />
        <attr name="bottomLineHeight" format="dimension" />
        <attr name="selectedBackgroundColor" format="reference" />
    </declare-styleable>
    <declare-styleable name="CirclePercentView">

        <attr name="circle_BgColor" format="reference" />

        <attr name="circle_ProgressColor" format="reference" />

        <attr name="circle_Radius" format="integer" />
        <!--渐变开始颜色-->
        <attr name="circle_StartColor" format="reference" />
        <!--渐变结束颜色-->
        <attr name="circle_EndColor" format="reference" />
        <!--是否渐变-->
        <attr name="circle_IsGradient" format="boolean" />
    </declare-styleable>
</resources>