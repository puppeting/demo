                android:paddingTop="5dp"
                android:paddingEnd="8dp"
                android:paddingBottom="5dp"
                android:singleLine="true"
                android:text=""
                android:hint="发布日期"
                android:textColor="@color/color_text_title"
                android:textSize="@dimen/sp_12"
